.title {
  font-weight: bold;
  font-size: 14px;
}

image.avatar {
  width: 88rpx;
  height: 88rpx;
  border-radius: 6rpx;
}

image.icon {
  width: 32rpx;
  height: 32rpx;
}

form.id button {
  background-color: transparent;
  text-align: left;
  line-height: 1.5;
  border-radius: 0 !important;
}

form.id button:hover {
  background-color: transparent;
}


button.border-less::after {
  border: none;
}